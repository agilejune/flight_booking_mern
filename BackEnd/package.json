{
  "name": "NumeroUnTravels",
  "version": "1.0.1",
  "description": "NumeroUnTravels",
  "main": "dist/server.js",
  "scripts": {
    "start": "nodemon dist/server.js",
    "build": "tsc -p . && ncp src/graphql/schemas/ dist/graphql/schemas",
    "build:dev": "npm run build:dev",
    "start:dev": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql",
    "db:create": "sequelize-cli db:create",
    "setup-db": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --debug",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "npx sequelize-cli migration:create --name modify_add_new_fields"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "graphql",
    "apollo",
    "express"
  ],
  "author": "NumeroUnTravels",
  "license": "MIT",
  "dependencies": {
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.6.7",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "graphql": "^15.8.0",
    "graphql-constraint-directive": "^3.1.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.26",
    "graphql-request": "^4.2.0",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.2.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "ncp": "^2.0.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.15",
    "npx": "^10.2.2",
    "path": "^0.12.7",
    "paystack": "^2.0.1",
    "pdf-creator-node": "^2.3.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "phantomjs-prebuilt": "^2.1.16",
    "randomstring": "^1.2.2",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1",
    "socket.io": "^4.5.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "uuidv4": "^6.2.13",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.29",
    "@types/paystack": "^2.0.1",
    "@types/validator": "^13.7.2"
  }
}
